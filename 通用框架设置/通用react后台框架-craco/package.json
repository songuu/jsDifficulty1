  "gitHooks": {
    "pre-commit": "lint-staged --allow-empty",
    "commit-msg": "commitlint -E GIT_PARAMS"
  },
  "lint-staged": {
    "**/*.{js,jsx,json,css,scss,less,html,md}": [
      "prettier --write"
    ],
    "src/**/*.{ts,tsx}": [
      "prettier --parser=typescript --write"
    ],
    "src/**/*.{ts,tsx,js,jsx}": [
      "eslint --fix"
    ]
  },
  "prettier": {
    "arrowParens": "avoid",
    "trailingComma": "es5",
    "singleQuote": true,
    "semi": true,
    "tabWidth": 2
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  },
  "antdTheme": {}
